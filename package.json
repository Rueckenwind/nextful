{
  "name": "nextful",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "NODE_ENV=production next build && next export",
    "test": "npm run lint",
    "lint": "npm run lint:css && npm run lint:js",
    "lint-fix": "npm run lint:css-fix && npm run lint:js-fix",
    "lint:css": "stylelint \"**/*.css\" --formatter verbose",
    "lint:css-fix": "stylelint \"**/*.css\" --formatter verbose --fix",
    "lint:js": "eslint \"**/*.js\"",
    "lint:js-fix": "eslint \"**/*.js\" --fix",
    "lint:html": "htmlhint out",
    "minify-html": "html-minifier --input-dir out --output-dir out --file-ext html --minify-css",
    "favicons-generate": "real-favicon generate faviconDescription.json faviconData.json static/assets/images/favicons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schoenwaldnils/gsdb.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/schoenwaldnils/gsdb/issues"
  },
  "homepage": "https://github.com/schoenwaldnils/gsdb#readme",
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@emotion/styled": "10.0.10",
    "@zeit/next-css": "1.0.1",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link-http": "1.5.14",
    "autoprefixer": "9.5.0",
    "babel-plugin-emotion": "10.0.9",
    "babel-plugin-inline-react-svg": "1.0.1",
    "contentful": "7.5.0",
    "dotenv": "7.0.0",
    "eslint": "5.15.3",
    "eslint-config-schoenwaldnils": "1.0.2",
    "graphql": "14.2.0",
    "graphql-tag": "2.10.1",
    "html-minifier": "3.5.21",
    "html-react-parser": "0.6.3",
    "htmlhint": "0.11.0",
    "isomorphic-fetch": "2.2.1",
    "next": "8.0.3",
    "normalize.css": "8.0.1",
    "postcss-calc": "7.0.1",
    "postcss-custom-media": "7.0.7",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-reporter": "6.0.1",
    "postcss-url": "8.0.0",
    "prop-types": "15.7.2",
    "raw-loader": "2.0.0",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "stylelint": "9.10.1",
    "stylelint-config-schoenwaldnils": "2.1.1"
  },
  "devDependencies": {
    "cli-real-favicon": "0.0.7",
    "husky": "1.3.1",
    "lint-staged": "8.1.5"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "**/*.css": [
      "stylelint --formatter verbose",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
