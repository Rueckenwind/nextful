{
  "extends": "eslint-config-schoenwaldnils",
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "react/no-unescaped-entities": ["error", {"forbid": [">", "'", "}"]}],
    "linebreak-style": ["error", "unix"],
    "react/destructuring-assignment": 0,
    "import/prefer-default-export": 0,
    "no-multi-str": 0
  }
}
